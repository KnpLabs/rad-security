{
    "name": "knplabs/rad-security",
    "description": "Provide RAD security components",
    "license": "MIT",
    "authors": [
        {
            "name": "KNP Labs",
            "email": "hello@knplabs.com"
        }
    ],
    "require": {
        "symfony/security": "~2.6"
    },
    "require-dev": {
        "bossa/phpspec2-expect": "~1.0",
        "pedrotroller/php-cs-custom-fixer": "~1.0@dev",
        "phpspec/phpspec": "~2.1",
        "symfony/dependency-injection": "~2.6",
        "symfony/event-dispatcher": "~2.6"
    },
    "suggest": {
        "knplabs/rad-resource-resolver": "Allows to check authorization against resources resolved from the routing."
    },
    "autoload": {
        "psr-0": {
            "Knp\\Rad\\Security": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
